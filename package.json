{
  "name": "fem-social-proof-section",
  "version": "1.0.0",
  "description": "Solution for the Front End Mentor challenge",
  "main": "index.js",
  "scripts": {
    "dev": "sass src/sass/main.scss src/css/style.css --watch",
    "compile:sass": "sass src/sass/main.scss src/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.comp.css -o src/css/style.prefix.css",
    "compress:css": "sass --style=compressed src/css/style.prefix.css src/css/style.css",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MachochinLopez/fem-social-proof-section.git"
  },
  "author": "MachochinLopez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MachochinLopez/fem-social-proof-section/issues"
  },
  "homepage": "https://github.com/MachochinLopez/fem-social-proof-section#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.5"
  }
}
